{
	"name": "ModManager",
	"version": "1.0.0",
	"author": "Atampy26",
	"private": true,
	"description": "Mod manager for the Simple Mod Framework.",
	"main": "src/electron.cjs",
	"type": "module",
	"scripts": {
		"dev": "cross-env NODE_ENV=dev npm run dev:all",
		"dev:all": "concurrently -n=svelte,electron -c='#ff3e00',blue \"npm run dev:svelte\" \"npm run dev:electron\"",
		"dev:svelte": "svelte-kit dev",
		"dev:electron": "electron src/electron.cjs",
		"build": "cross-env NODE_ENV=production npm run build:svelte && npm run build:electron",
		"build:svelte": "svelte-kit build",
		"build:electron": "electron-builder -w --dir --config build.config.json",
		"start": "svelte-kit start"
	},
	"engines": {
		"npm": ">=7",
		"pnpm": "use npm - https://github.com/FractalHQ/sveltekit-electron/issues/12#issuecomment-1068399385",
		"yarn": "use npm - https://github.com/FractalHQ/sveltekit-electron/issues/12#issuecomment-1068399385"
	},
	"browserslist": [
		"Chrome 89"
	],
	"dependencies": {
		"@electron/remote": "^2.0.9",
		"@fortawesome/free-regular-svg-icons": "^6.3.0",
		"@fortawesome/free-solid-svg-icons": "^6.4.0",
		"adm-zip": "^0.5.10",
		"ajv": "^8.12.0",
		"autoprefixer": "^10.4.14",
		"axios": "^1.3.4",
		"carbon-components-svelte": "^0.73.4",
		"carbon-icons-svelte": "^11.4.0",
		"chalk": "^5.2.0",
		"electron-context-menu": "^3.6.1",
		"electron-reloader": "^1.2.3",
		"electron-serve": "^1.1.0",
		"electron-window-state": "^5.0.3",
		"filtrex": "^3.0.0",
		"fs-extra": "^11.1.1",
		"hash-wasm": "^4.9.0",
		"json5": "^2.2.3",
		"klaw-sync": "^6.0.0",
		"lodash.chunk": "^4.2.0",
		"lodash.get": "^4.4.2",
		"lodash.isequal": "^4.5.0",
		"lodash.memoize": "^4.1.2",
		"lodash.mergewith": "^4.6.2",
		"marked": "^4.3.0",
		"md5": "^2.3.0",
		"postcss": "^8.4.21",
		"rehype-stringify": "^9.0.3",
		"sanitize-html": "^2.10.0",
		"semver": "^7.3.8",
		"shiki": "^0.14.1",
		"svelte-fa": "^3.0.3",
		"svelte-sortable-list": "^1.1.0",
		"svelte-tippy": "^1.3.2",
		"tailwindcss": "^3.2.7",
		"tippy.js": "^6.3.7",
		"unified": "^10.1.2",
		"uuid": "^9.0.0"
	},
	"devDependencies": {
		"@sveltejs/adapter-static": "^2.0.1",
		"@sveltejs/kit": "next",
		"@types/adm-zip": "^0.5.0",
		"@types/electron-window-state": "^5.0.0",
		"@types/lodash.isequal": "^4.5.6",
		"@types/lodash.memoize": "^4.1.7",
		"@types/lodash.mergewith": "^4.6.7",
		"@types/marked": "^4.0.8",
		"@types/sanitize-html": "^2.9.0",
		"@types/uuid": "^9.0.1",
		"@typescript-eslint/eslint-plugin": "^5.56.0",
