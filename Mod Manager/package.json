		"electron-connect": "^0.6.3",
		"electron-packager": "^17.1.1",
		"electron-updater": "^5.3.0",
		"eslint": "^8.36.0",
		"eslint-plugin-svelte": "^2.22.0",
		"npm-run-all": "^4.1.5",
		"prettier": "^2.8.6",
		"prettier-eslint": "^15.0.1",
		"prettier-plugin-svelte": "^2.10.0",
		"sass": "^1.59.3",
		"svelte": "~3.57.0",
		"svelte-check": "^3.1.4",
		"svelte-preprocess": "^5.0.3",
		"typescript": "^5.0.2",
		"vite": "^4.2.1"
	}
